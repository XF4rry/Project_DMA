{
  "hosting": {
    "public": "index",
    "redirects": [
      {
        "source": "/song",
        "destination": "/song/song.html",
        "type": 301
      },
      {
        "source": "/select",
        "destination": "/selectCat/select.html",
        "type": 301
      },
      {
        "source": "/monthly",
        "destination": "/monthly/monthly.html",
        "type": 301
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "database": {
    "rules": "database.rules.json"
  }
}
